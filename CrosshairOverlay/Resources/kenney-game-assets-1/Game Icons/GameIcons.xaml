<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:sys="clr-namespace:System;assembly=System">

	<sys:Uri x:Key="Uri Return">pack://application:,,,/Resources/kenney-game-assets-1/Game Icons/return.png</sys:Uri>
	<sys:Uri x:Key="Uri Save">pack://application:,,,/Resources/kenney-game-assets-1/Game Icons/save.png</sys:Uri>
	<sys:Uri x:Key="Uri Cross">pack://application:,,,/Resources/kenney-game-assets-1/Game Icons/cross.png</sys:Uri>
	<sys:Uri x:Key="Uri Wrench">pack://application:,,,/Resources/kenney-game-assets-1/Game Icons/wrench.png</sys:Uri>
	
	<BitmapImage x:Key="Bitmap Return" BaseUri="{ StaticResource 'Uri Return' }" />
	<BitmapImage x:Key="Bitmap Save" BaseUri="{ StaticResource 'Uri Save' }" />
	<BitmapImage x:Key="Bitmap Cross" BaseUri="{ StaticResource 'Uri Cross' }" />
	<BitmapImage x:Key="Bitmap Wrench" BaseUri="{ StaticResource 'Uri Wrench' }" />
	
</ResourceDictionary>
